          <tr class="hover:bg-nerv-purple-mid/30">
            <td class="py-3 px-4 text-terminal-white">Command Core</td>
            <td class="py-3 px-4 text-terminal-cyan">Sidebar Navigation</td>
            <td class="py-3 px-4 text-terminal-gray">Main navigation menu on the left side</td>
          </tr>
          <tr class="hover:bg-nerv-purple-mid/30">
            <td class="py-3 px-4 text-terminal-white">MAGI System</td>
            <td class="py-3 px-4 text-terminal-cyan">Application Status</td>
            <td class="py-3 px-4 text-terminal-gray">System health and connection status indicator</td>
